<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="testTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>


        <!-- 为模型生成序列化方法-->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>

        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL
            和 NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="${jdbc.driverClass}"
                        connectionURL="${jdbc.connectionURL}"
                        userId="${jdbc.userId}"
                        password="${jdbc.password}">
            <property name="useInformationSchema" value="true"/>
        </jdbcConnection>


        <!-- targetProject:生成PO类的位置 -->
        <javaModelGenerator
                targetPackage="com.tujietg.template.dao"
                targetProject="/Users/tujietg/IdeaProjects/springboot-template/src/main/java/com/tujietg/template/dao"/>


        <!-- targetPackage：mapper接口生成的位置 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.tujietg.template.mbg.mapper"
                             targetProject="/Users/tujietg/IdeaProjects/springboot-template/src/main/java/com/tujietg/template/mbg/mapper"/>

        <!-- targetProject:mapper映射文件生成的位置  mapper.xml-->
        <sqlMapGenerator
                targetPackage="com.tujietg.template.mbg.mapper"
                targetProject="/Users/tujietg/IdeaProjects/springboot-template/src/main/resources"/>
